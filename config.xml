<?xml version='1.0' encoding='utf-8'?>
<widget id="com.webzin.paypal" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Ngpay</name>
    <description>
        A blank PhoneGap app.
    </description>
    <author email="sameer@webzin.in" href="https://www.webzin.in">
        PhoneGap Team
    </author>
	<preference name="android-minSdkVersion" value="16" />
	<gap:plugin name="com.paypal.cordova.mobilesdk" source="npm" />
    <content src="index.html" />
    <access origin="https://*.bootstrapcdn.com" />
	<access origin="https://*.cloudflare.com" />
	<access origin="https://paypalobjects.com/*" />
	<access origin="https://*.sandbox.paypal.com" />
	<access origin="https://sandbox.paypal.com/*" />
  <access origin="https://maxcdn.bootstrapcdn.com/*" />
  <access origin="https://cdnjs.cloudflare.com/*" />
  
    <access origin="*" />
	<!-- A wildcard can be used to whitelist the entire network,
     over HTTP and HTTPS.
     *NOT RECOMMENDED* -->
<allow-navigation href="*" />
<allow-navigation href="http://*/*" />
<allow-intent href="https://*/*" />
</widget>
